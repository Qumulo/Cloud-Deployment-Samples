        "RegionMap": {
            "ca-central-1": {
                "AMI": "REPLACE_ME"
            },
            "eu-central-1": {
                "AMI": "REPLACE_ME"
            },
            "eu-west-1": {
                "AMI": "REPLACE_ME"
            },
            "eu-west-2": {
                "AMI": "REPLACE_ME"
            },
            "eu-west-3": {
                "AMI": "REPLACE_ME"
            },
            "us-east-1": {
                "AMI": "REPLACE_ME"
            },
            "us-east-2": {
                "AMI": "REPLACE_ME"
            },
            "us-west-1": {
                "AMI": "REPLACE_ME"
            },
            "us-west-2": {
                "AMI": "REPLACE_ME"
            }
        }
    },
    "Metadata": {
        "AWS::CloudFormation::Interface": {
            "ParameterGroups": [
                {
                    "Label": {
                        "default": "Amazon EC2 Configuration"
                    },
                    "Parameters": [
                        "InstanceType",
                        "KeyName"
                    ]
                },
                {
                    "Label": {
                        "default": "Network Configuration"
                    },
                    "Parameters": [
                        "VpcId",
                        "SubnetId"
                    ]
                },
                {
                    "Label": {
                        "default": "QF2 Configuration"
                    },
                    "Parameters": [
                        "ClusterName",
                        "AdminPassword"
                    ]
                }
            ],
            "ParameterLabels": {
                "AdminPassword": {
                    "default": "QF2 administrator password"
                },
                "ClusterName": {
                    "default": "QF2 cluster name"
                },
                "InstanceType": {
                    "default": "EC2 instance type"
                },
                "KeyName": {
                    "default": "SSH key-pair name"
                },
                "SubnetId": {
                    "default": "Subnet ID in the VPC"
                },
                "VpcId": {
                    "default": "VPC ID"
                }
            }
        }
    },
    "Outputs": {
        "ClusterPrivateIPs": {
            "Description": "Copy and paste this list into the QF2 Cluster Creation Screen",
            "Value": {
                "Fn::Join": [
                    ", ",
                    [
                        {
                            "Fn::GetAtt": [
                                "QF2Node1",
                                "PrivateIp"
                            ]
                        },
                        {
                            "Fn::GetAtt": [
                                "QF2Node2",
                                "PrivateIp"
                            ]
                        },
                        {
                            "Fn::GetAtt": [
                                "QF2Node3",
                                "PrivateIp"
                            ]
                        },
                        {
                            "Fn::GetAtt": [
                                "QF2Node4",
                                "PrivateIp"
                            ]
                        }
                    ]
                ]
            }
        },
        "InstanceId": {
            "Description": "Copy and paste this instance ID into the QF2 Cluster Creation Screen.",
            "Value": {
                "Ref": "QF2Node1"
            }
        },
        "LinkToManagement": {
            "Description": "Click to launch the QF2 Admin Console",
            "Value": {
                "Fn::Join": [
                    "",
                    [
                        "https://",
                        {
                            "Fn::GetAtt": [
                                "QF2Node1",
                                "PrivateIp"
                            ]
                        }
                    ]
                ]
            }
        },
        "QumuloKnowledgeBase": {
            "Description": "Qumulo Knowledge Base for QF2 in public clouds",
            "Value": "https://care.qumulo.com/hc/en-us/sections/115003388428-QF2-IN-THE-CLOUD"
        }
    },
    "Parameters": {
        "AdminPassword": {
            "Description": "QF2 administrator account password",
            "Type": "String"
        },
        "ClusterName": {
            "Description": "QF2 cluster name",
            "Type": "String"
        },
        "InstanceType": {
            "AllowedValues": [
                "m4.xlarge",
                "m4.2xlarge",
                "m4.4xlarge",
                "m4.10xlarge",
                "m4.16xlarge",
                "m5.xlarge",
                "m5.2xlarge",
                "m5.4xlarge",
                "m5.12xlarge",
                "m5.24xlarge"
            ],
            "ConstraintDescription": "Must be a Qumulo supported EC2 instance type.",
            "Default": "m4.4xlarge",
            "Description": "EC2 instance type for QF2 node",
            "Type": "String"
        },
        "KeyName": {
            "Description": "Name of an existing EC2 KeyPair to enable SSH access to the node",
            "Type": "AWS::EC2::KeyPair::KeyName"
        },
        "SubnetId": {
            "ConstraintDescription": "Must be the ID of an existing Subnet.",
            "Description": "ID of the Subnet in which to deploy QF2.",
            "Type": "AWS::EC2::Subnet::Id"
        },
        "VpcId": {
            "ConstraintDescription": "Must be the ID of an existing VPC.",
            "Description": "ID of the VPC in which to deploy QF2.",
            "Type": "AWS::EC2::VPC::Id"
        }
    },
